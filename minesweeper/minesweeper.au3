#include <WindowsConstants.au3>
#include <GUIConstantsEx.au3>
#include <GDIPlus.au3>
#include <File.au3>

Opt("GUIOnEventMode", 1)
Opt("MouseCoordMode", 2)

Dim $BIN_SRC[14]
$BIN_SRC[0] = ""
$BIN_SRC[0] &= ""
$BIN_SRC[1] = ""
$BIN_SRC[1] &= ""
$BIN_SRC[2] = ""
$BIN_SRC[2] &= ""
$BIN_SRC[3] = ""
$BIN_SRC[3] &= ""
$BIN_SRC[4] = ""
$BIN_SRC[4] &= ""
$BIN_SRC[5] = ""
$BIN_SRC[5] &= ""
$BIN_SRC[6] = ""
$BIN_SRC[6] &= ""
$BIN_SRC[7] = ""
$BIN_SRC[7] &= ""
$BIN_SRC[8] = ""
$BIN_SRC[8] &= ""
$BIN_SRC[9] = ""
$BIN_SRC[9] &= ""
$BIN_SRC[10] = ""
$BIN_SRC[10] &= ""
$BIN_SRC[11] = ""
$BIN_SRC[11] &= ""
$BIN_SRC[12] = ""
$BIN_SRC[12] &= ""
$BIN_SRC[13] = ""
$BIN_SRC[13] &= ""

; Game
Dim $GAME_PATH[2]
$GAME_PATH[0] = @AppDataDir & "\Minesweeper"
$GAME_PATH[1] = @AppDataDir & "\Minesweeper\"
$GAME_SETTINGS = $GAME_PATH[1] & "\settings.ini"
$GAME_STATS = $GAME_PATH[1] & "\stats.ini"

$FIELD_SIZE = 28
$FIELD_DISTANCE = 2
Global $LAST_HOVER_X = 1
Global $LAST_HOVER_Y = 1

$MOVE = 0
$FIRSTMOVE = 1

; Default
$DEFAULT_DIFFICULTY = 2
$DEFAULT_CUSTOM_WIDTH = 16
$DEFAULT_CUSTOM_HEIGHT = 16
$DEFAULT_CUSTOM_MINES = 40
$DEFAULT_COLOR_BACKGROUND = -12320768
$DEFAULT_COLOR_FIELDS = -2236963
$DEFAULT_COLOR_NUMBERS = -13421773
$DEFAULT_COLOR_BOMBS = -16777216
$DEFAULT_COLOR_HIDE = -8947849
$DEFAULT_COLOR_HOVER = -10066330

; Settings
$SETTING_DELETE_GRAPHICS = 0
$SETTING_DELETE_SETTINGS = 0

If Not FileExists($GAME_PATH[0]) Then DirCreate($GAME_PATH[0])
If $SETTING_DELETE_GRAPHICS Then
   For $i = 0 To UBound($BIN_SRC)-1
	  If FileExists($GAME_PATH[1] & $BIN_SRC[$i] & ".bmp") Then FileDelete($GAME_PATH[1] & $BIN_SRC[$i] & ".bmp")
   Next
EndIf
If $SETTING_DELETE_SETTINGS Then
   If FileExists($GAME_SETTINGS) Then FileDelete($GAME_SETTINGS)
EndIf
If Not FileExists($GAME_SETTINGS) Then
   IniWrite($GAME_SETTINGS, "GAME", "difficulty", $DEFAULT_DIFFICULTY)
   IniWrite($GAME_SETTINGS, "GAME", "custom_width", $DEFAULT_CUSTOM_WIDTH)
   IniWrite($GAME_SETTINGS, "GAME", "custom_height", $DEFAULT_CUSTOM_HEIGHT)
   IniWrite($GAME_SETTINGS, "GAME", "custom_mines", $DEFAULT_CUSTOM_MINES)
   IniWrite($GAME_SETTINGS, "GAME", "color_background", $DEFAULT_COLOR_BACKGROUND)
   IniWrite($GAME_SETTINGS, "GAME", "color_fields", $DEFAULT_COLOR_FIELDS)
   IniWrite($GAME_SETTINGS, "GAME", "color_numbers", $DEFAULT_COLOR_NUMBERS)
   IniWrite($GAME_SETTINGS, "GAME", "color_bombs", $DEFAULT_COLOR_BOMBS)
   IniWrite($GAME_SETTINGS, "GAME", "color_hide", $DEFAULT_COLOR_HIDE)
   IniWrite($GAME_SETTINGS, "GAME", "color_hover", $DEFAULT_COLOR_HOVER)

   IniWrite($GAME_STATS, "STATS", "wins_custom", 0)
   IniWrite($GAME_STATS, "STATS", "loses_custom", 0)
   IniWrite($GAME_STATS, "STATS", "wins_easy", 0)
   IniWrite($GAME_STATS, "STATS", "loses_easy", 0)
   IniWrite($GAME_STATS, "STATS", "wins_normal", 0)
   IniWrite($GAME_STATS, "STATS", "loses_normal", 0)
   IniWrite($GAME_STATS, "STATS", "wins_difficult", 0)
   IniWrite($GAME_STATS, "STATS", "loses_difficult", 0)
EndIf

$SETTING_DIFFICULTY = IniRead($GAME_SETTINGS, "GAME", "difficulty", $DEFAULT_DIFFICULTY)
$SETTING_CUSTOM_WIDTH = IniRead($GAME_SETTINGS, "GAME", "custom_width", $DEFAULT_CUSTOM_WIDTH)
$SETTING_CUSTOM_HEIGHT = IniRead($GAME_SETTINGS, "GAME", "custom_height", $DEFAULT_CUSTOM_HEIGHT)
$SETTING_CUSTOM_MINES = IniRead($GAME_SETTINGS, "GAME", "custom_mines", $DEFAULT_CUSTOM_MINES)
$SETTING_COLOR_BACKGROUND = IniRead($GAME_SETTINGS, "GAME", "color_background", $DEFAULT_COLOR_BACKGROUND)
$SETTING_COLOR_FIELDS = IniRead($GAME_SETTINGS, "GAME", "color_fields", $DEFAULT_COLOR_FIELDS)
$SETTING_COLOR_NUMBERS = IniRead($GAME_SETTINGS, "GAME", "color_numbers", $DEFAULT_COLOR_NUMBERS)
$SETTING_COLOR_BOMBS = IniRead($GAME_SETTINGS, "GAME", "color_bombs", $DEFAULT_COLOR_BOMBS)
$SETTING_COLOR_HIDE = IniRead($GAME_SETTINGS, "GAME", "color_hide", $DEFAULT_COLOR_HIDE)
$SETTING_COLOR_HOVER = IniRead($GAME_SETTINGS, "GAME", "color_hover", $DEFAULT_COLOR_HOVER)

$STATS_WINS_CUSTOM = IniRead($GAME_STATS, "STATS", "wins_custom", 0)
$STATS_LOSES_CUSTOM = IniRead($GAME_STATS, "STATS", "loses_custom", 0)
$STATS_WINS_EASY = IniRead($GAME_STATS, "STATS", "wins_easy", 0)
$STATS_LOSES_EASY = IniRead($GAME_STATS, "STATS", "loses_easy", 0)
$STATS_WINS_NORMAL = IniRead($GAME_STATS, "STATS", "wins_normal", 0)
$STATS_LOSES_NORMAL = IniRead($GAME_STATS, "STATS", "loses_normal", 0)
$STATS_WINS_DIFFICULT = IniRead($GAME_STATS, "STATS", "wins_difficult", 0)
$STATS_LOSES_DIFFICULT = IniRead($GAME_STATS, "STATS", "loses_difficult", 0)

Switch $SETTING_DIFFICULTY
   Case 0
	  $FLOOR_WIDTH = $SETTING_CUSTOM_WIDTH
	  $FLOOR_HEIGHT = $SETTING_CUSTOM_HEIGHT
	  $FLOOR_MINES = $SETTING_CUSTOM_MINES
	  If $FLOOR_WIDTH < 10 Then $FLOOR_WIDTH = 10
	  If $FLOOR_HEIGHT < 10 Then $FLOOR_HEIGHT = 10
	  If $FLOOR_MINES < $FLOOR_WIDTH * $FLOOR_HEIGHT * 0.1 Then $FLOOR_MINES = $FLOOR_WIDTH * $FLOOR_HEIGHT * 0.1
	  If $FLOOR_WIDTH > 50 Then $FLOOR_WIDTH = 50
	  If $FLOOR_HEIGHT > 40 Then $FLOOR_HEIGHT = 40
	  If $FLOOR_MINES > $FLOOR_WIDTH * $FLOOR_HEIGHT * 0.9 Then $FLOOR_MINES = $FLOOR_WIDTH * $FLOOR_HEIGHT * 0.9
   Case 1
		$FLOOR_WIDTH = 9
		$FLOOR_HEIGHT = 9
		$FLOOR_MINES = 10
   Case 3
	  $FLOOR_WIDTH = 30
	  $FLOOR_HEIGHT = 16
	  $FLOOR_MINES = 99
   Case Else
	  $FLOOR_WIDTH = 16
	  $FLOOR_HEIGHT = 16
	  $FLOOR_MINES = 40
EndSwitch

Dim $FLOOR[$FLOOR_WIDTH + 2][$FLOOR_HEIGHT + 2], $STATE[$FLOOR_WIDTH + 2][$FLOOR_HEIGHT + 2]
Dim $IMAGE[UBound($BIN_SRC)]

_GDIPlus_Startup()
$GDI_BRUSH = _GDIPlus_BrushCreateSolid($SETTING_COLOR_BACKGROUND)

For $I = 0 To UBound($IMAGE)-1
   If Not FileExists($GAME_PATH[1] & $i & ".bmp") Then
	  Local $hFileOut = FileOpen($GAME_PATH[1] & $I & ".bmp", 18)
	  FileWrite($hFileOut, Binary($BIN_SRC[$I]))
	  FileClose($hFileOut)
   EndIf
   $IMAGE[$I] = _GDIPlus_BitmapCreateFromFile($GAME_PATH[1] & $I & ".bmp")
   For $X = 0 To 27
	  For $Y = 0 To 27
		 If _GDIPLUS_BITMAPGETPIXEL($IMAGE[$I], $X, $Y) = "4278191872" Then
			_GDIPLUS_BITMAPSETPIXEL($IMAGE[$I], $X, $Y, $SETTING_COLOR_FIELDS)
		 ElseIf _GDIPLUS_BITMAPGETPIXEL($IMAGE[$I], $X, $Y) = "4278583296" Then
			_GDIPLUS_BITMAPSETPIXEL($IMAGE[$I], $X, $Y, $SETTING_COLOR_NUMBERS)
		 ElseIf _GDIPLUS_BITMAPGETPIXEL($IMAGE[$I], $X, $Y) = "4278190087" Then
			_GDIPLUS_BITMAPSETPIXEL($IMAGE[$I], $X, $Y, $SETTING_COLOR_BOMBS)
		 ElseIf _GDIPLUS_BITMAPGETPIXEL($IMAGE[$I], $X, $Y) = "4278650631" Then
			_GDIPLUS_BITMAPSETPIXEL($IMAGE[$I], $X, $Y, $SETTING_COLOR_HIDE)
		 ElseIf _GDIPLUS_BITMAPGETPIXEL($IMAGE[$I], $X, $Y) = "4278191879" Then
			_GDIPLUS_BITMAPSETPIXEL($IMAGE[$I], $X, $Y, $SETTING_COLOR_HOVER)
		 EndIf
	  Next
   Next
Next

$GUI_WIDTH = 10 + ($FLOOR_WIDTH + 1) * ($FIELD_SIZE + $FIELD_DISTANCE) - $FIELD_DISTANCE
$GUI_HEIGHT = 10 + ($FLOOR_HEIGHT + 1) * ($FIELD_SIZE + $FIELD_DISTANCE) - $FIELD_DISTANCE
$GUI_HWND = GUICreate("Minesweeper", $GUI_WIDTH, $GUI_HEIGHT, -1, -1)
$GUI_GRAPHIC = _GDIPlus_GraphicsCreateFromHWND($GUI_HWND)
GUIRegisterMsg($WM_PAINT, "_Paint")
GUISetOnEvent($GUI_EVENT_CLOSE, "_Exit")
GUISetOnEvent($GUI_EVENT_PRIMARYDOWN, "_PrimaryClick")
GUISetOnEvent($GUI_EVENT_SECONDARYDOWN, "_SecondaryClick")
GUISetOnEvent($GUI_EVENT_MOUSEMOVE, "_Hover")
GUISetState(@SW_SHOW, $GUI_HWND)

Func _PrimaryClick()
   $POS = MouseGetPos()
   $X2 = Mod($POS[0] + 10, $FIELD_SIZE + $FIELD_DISTANCE)
   If $X2 >= 0 And $X2 <= 27 Then
	  $X = Floor(($POS[0] + 10) / ($FIELD_SIZE + $FIELD_DISTANCE))
	  $Y2 = Mod($POS[1] + 10, $FIELD_SIZE + $FIELD_DISTANCE)
	  If $Y2 >= 0 And $X2 <= 27 Then
		 $Y = Floor(($POS[1] + 10) / ($FIELD_SIZE + $FIELD_DISTANCE))
		 If $X > 0 And $X <= $FLOOR_WIDTH And $Y > 0 And $Y <= $FLOOR_HEIGHT Then
			_Show($X,$Y)
		 EndIf
	  EndIf
   EndIf
EndFunc

Func _SecondaryClick()
   $POS = MouseGetPos()
   $X = Mod($POS[0] + 10, $FIELD_SIZE + $FIELD_DISTANCE)
   $Y = Mod($POS[1] + 10, $FIELD_SIZE + $FIELD_DISTANCE)
   If $X > -1 And $X < 28 And $Y > -1 And $Y < 28 Then
	  $X = Floor(($POS[0] + 10) / ($FIELD_SIZE + $FIELD_DISTANCE))
	  $Y = Floor(($POS[1] + 10) / ($FIELD_SIZE + $FIELD_DISTANCE))
	  If $X > 0 And $X < $FLOOR_WIDTH+1 And $Y > 0 And $Y < $FLOOR_HEIGHT+1 Then
		 If $STATE[$X][$Y] = 0 Then
			$STATE[$X][$Y] = 2
			_GDIPlus_GraphicsDrawImageRect($GUI_GRAPHIC, $IMAGE[11], -10 + $LAST_HOVER_X * ($FIELD_SIZE + $FIELD_DISTANCE), -10 + $LAST_HOVER_Y * ($FIELD_SIZE + $FIELD_DISTANCE), $FIELD_SIZE, $FIELD_SIZE)
		 ElseIf $STATE[$X][$Y] = 2 Then
			$STATE[$X][$Y] = 0
			_GDIPlus_GraphicsDrawImageRect($GUI_GRAPHIC, $IMAGE[10], -10 + $LAST_HOVER_X * ($FIELD_SIZE + $FIELD_DISTANCE), -10 + $LAST_HOVER_Y * ($FIELD_SIZE + $FIELD_DISTANCE), $FIELD_SIZE, $FIELD_SIZE)
		 EndIf
	  EndIf
   EndIf
EndFunc

Func _Show($X,$Y)
   If $X < 1 Or $X > $FLOOR_WIDTH Or $Y < 1 Or $Y > $FLOOR_HEIGHT Then Return
   If $FIRSTMOVE = 1 Then
	  For $I = 1 To $FLOOR_MINES
		 $RAND_X = Random(1, $FLOOR_WIDTH, 1)
		 $RAND_Y = Random(1, $FLOOR_HEIGHT, 1)
		 If $FLOOR[$RAND_X][$RAND_Y] <> 9 And $RAND_X <> $X And $RAND_Y <> $Y Then
			$FLOOR[$RAND_X][$RAND_Y] = 9
		 Else
			$I -= 1
		 EndIf
	  Next
	  For $I_X = 1 To $FLOOR_WIDTH
		 For $I_Y = 1 To $FLOOR_HEIGHT
			If $FLOOR[$I_X][$I_Y] <> 9 Then
			   $I_MINES = 0
			   If $I_X-1 > -1 Then
				  If $FLOOR[$I_X - 1][$I_Y] = 9 Then $I_MINES += 1
				  If $I_Y-1 > -1 Then
					 If $FLOOR[$I_X - 1][$I_Y - 1] = 9 Then $I_MINES += 1
				  EndIf
				  If $FLOOR[$I_X - 1][$I_Y + 1] = 9 Then $I_MINES += 1
			   EndIf
			   If $FLOOR[$I_X][$I_Y + 1] = 9 Then $I_MINES += 1
			   If $I_Y-1 > -1 Then
				  If $FLOOR[$I_X][$I_Y - 1] = 9 Then $I_MINES += 1
				  If $FLOOR[$I_X + 1][$I_Y - 1] = 9 Then $I_MINES += 1
			   EndIf
			   If $FLOOR[$I_X + 1][$I_Y] = 9 Then $I_MINES += 1
			   If $FLOOR[$I_X + 1][$I_Y + 1] = 9 Then $I_MINES += 1
			   $FLOOR[$I_X][$I_Y] = $I_MINES
			EndIf
		 Next
	  Next
	  $FIRSTMOVE = 0
   EndIf

   If Not $STATE[$X][$Y] Then
	  If $FLOOR[$X][$Y] <> 9 Then
		 $MOVE += 1
	  EndIf
	  $STATE[$X][$Y] = 1
	  _GDIPlus_GraphicsDrawImageRect($GUI_GRAPHIC, $IMAGE[$FLOOR[$X][$Y]], -10 + $X * ($FIELD_SIZE + $FIELD_DISTANCE), -10 + $Y * ($FIELD_SIZE + $FIELD_DISTANCE), $FIELD_SIZE, $FIELD_SIZE)

	  If $MOVE = ($FLOOR_WIDTH*$FLOOR_HEIGHT)-$FLOOR_MINES Then
		 MsgBox(0,"Gewonnen","Herzlichen Glückwunsch! Sie haben das Spiel gewonnen." & @CRLF &  "Das Spiel wird nun neugestartet.")

		 Switch $SETTING_DIFFICULTY
			Case 0
			   $STATS_WINS_CUSTOM += 1
			Case 1
			   $STATS_WINS_EASY += 1
			Case 3
			   $STATS_WINS_DIFFICULT += 1
			Case Else
			   $STATS_WINS_NORMAL += 1
		 EndSwitch

		 $FIRSTMOVE = 1
		 $MOVE = 0
		 For $I_X = 1 To $FLOOR_WIDTH
			For $I_Y = 1 To $FLOOR_WIDTH
			   $STATE[$I_X][$I_Y] = 0
			   $FLOOR[$I_X][$I_Y] = 1
			   _GDIPlus_GraphicsDrawImageRect($GUI_GRAPHIC, $IMAGE[10], -10 + $I_X * ($FIELD_SIZE + $FIELD_DISTANCE), -10 + $I_Y * ($FIELD_SIZE + $FIELD_DISTANCE), $FIELD_SIZE, $FIELD_SIZE)
			Next
		 Next
		 Return
	  Else
		 If $FLOOR[$X][$Y] = 0 Then
			_Show($X-1,$Y-1)
			_Show($X-1,$Y+1)
			_Show($X-1,$Y)
			_Show($X,$Y-1)
			_Show($X+1,$Y-1)
			_Show($X+1,$Y)
			_Show($X,$Y+1)
			_Show($X+1,$Y+1)
		 ElseIf $FLOOR[$X][$Y] = 9 Then
			MsgBox(0,"Verloren","Sie haben eine Mine getroffen." & @CRLF &  "Das Spiel wird nun neugestartet.")

			Switch $SETTING_DIFFICULTY
			   Case 0
				  $STATS_LOSES_CUSTOM += 1
			   Case 1
				  $STATS_LOSES_EASY += 1
			   Case 3
				  $STATS_LOSES_DIFFICULT += 1
			   Case Else
				  $STATS_LOSES_NORMAL += 1
			EndSwitch
			$FIRSTMOVE = 1
			$MOVE = 0
			For $I_X = 1 To $FLOOR_WIDTH
			   For $I_Y = 1 To $FLOOR_WIDTH
				  $STATE[$I_X][$I_Y] = 0
				  $FLOOR[$I_X][$I_Y] = 1
				  _GDIPlus_GraphicsDrawImageRect($GUI_GRAPHIC, $IMAGE[10], -10 + $I_X * ($FIELD_SIZE + $FIELD_DISTANCE), -10 + $I_Y * ($FIELD_SIZE + $FIELD_DISTANCE), $FIELD_SIZE, $FIELD_SIZE)
			   Next
			Next
			Return
		 EndIf
	  EndIf
   EndIf
EndFunc

Func _Hover()
   $POS = MouseGetPos()
   $X = Mod($POS[0] + 10, $FIELD_SIZE + $FIELD_DISTANCE)
   $Y = Mod($POS[1] + 10, $FIELD_SIZE + $FIELD_DISTANCE)
   If $X > -1 And $X < 28 And $Y > -1 And $Y < 28 Then
	  $X = Floor(($POS[0] + 10) / ($FIELD_SIZE + $FIELD_DISTANCE))
	  $Y = Floor(($POS[1] + 10) / ($FIELD_SIZE + $FIELD_DISTANCE))
	  If $X > 0 And $X < $FLOOR_WIDTH+1 And $Y > 0 And $Y < $FLOOR_HEIGHT+1 Then
		 If $X <> $LAST_HOVER_X Or $Y <> $LAST_HOVER_Y Then
			If Not $STATE[$LAST_HOVER_X][$LAST_HOVER_Y] Then _GDIPlus_GraphicsDrawImageRect($GUI_GRAPHIC, $IMAGE[10], -10 + $LAST_HOVER_X * ($FIELD_SIZE + $FIELD_DISTANCE), -10 + $LAST_HOVER_Y * ($FIELD_SIZE + $FIELD_DISTANCE), $FIELD_SIZE, $FIELD_SIZE)
			   $LAST_HOVER_X = $X
			   $LAST_HOVER_Y = $Y
			   If Not $STATE[$X][$Y] Then _GDIPlus_GraphicsDrawImageRect($GUI_GRAPHIC, $IMAGE[12], -10 + $LAST_HOVER_X * ($FIELD_SIZE + $FIELD_DISTANCE), -10 + $LAST_HOVER_Y * ($FIELD_SIZE + $FIELD_DISTANCE), $FIELD_SIZE, $FIELD_SIZE)
			EndIf
		EndIf
	EndIf
EndFunc

Func _Paint($HWND, $MSG, $WPARAM, $LPARAM)
   _WinAPI_RedrawWindow($GUI_HWND, 0, 0, $RDW_UPDATENOW)
   _GDIPlus_GraphicsFillRect($GUI_GRAPHIC, 0, 0, $GUI_WIDTH, $GUI_HEIGHT, $GDI_BRUSH)
   _GDIPlus_GraphicsDrawImageRectRect($GUI_GRAPHIC, $IMAGE[13], 0, 100, 1, 101, 15, 15, $GUI_WIDTH - 30, $GUI_HEIGHT - 30)
   _GDIPlus_GraphicsDrawImageRect($GUI_GRAPHIC, $IMAGE[13], 0, 0, $GUI_WIDTH, $GUI_HEIGHT)
   For $X = 1 To $FLOOR_WIDTH
	  For $Y = 1 To $FLOOR_HEIGHT
		 If $STATE[$X][$Y] = 0 Then
			_GDIPlus_GraphicsDrawImageRect($GUI_GRAPHIC, $IMAGE[10], -10 + $X * ($FIELD_SIZE + $FIELD_DISTANCE), -10 + $Y * ($FIELD_SIZE + $FIELD_DISTANCE), $FIELD_SIZE, $FIELD_SIZE)
		 ElseIf $STATE[$X][$Y] = 2 Then
			_GDIPlus_GraphicsDrawImageRect($GUI_GRAPHIC, $IMAGE[11], -10 + $X * ($FIELD_SIZE + $FIELD_DISTANCE), -10 + $Y * ($FIELD_SIZE + $FIELD_DISTANCE), $FIELD_SIZE, $FIELD_SIZE)
		 Else
			_GDIPlus_GraphicsDrawImageRect($GUI_GRAPHIC, $IMAGE[$FLOOR[$X][$Y]], -10 + $X * ($FIELD_SIZE + $FIELD_DISTANCE), -10 + $Y * ($FIELD_SIZE + $FIELD_DISTANCE), $FIELD_SIZE, $FIELD_SIZE)
		 EndIf
	  Next
   Next
   _WinAPI_RedrawWindow($GUI_HWND, 0, 0, $RDW_VALIDATE)
EndFunc

Func _Exit()
   IniWrite($GAME_STATS,"STATS","wins_easy",$STATS_WINS_EASY)
   IniWrite($GAME_STATS,"STATS","loses_easy",$STATS_LOSES_EASY)
   IniWrite($GAME_STATS,"STATS","wins_normal",$STATS_WINS_NORMAL)
   IniWrite($GAME_STATS,"STATS","loses_normal",$STATS_LOSES_NORMAL)
   IniWrite($GAME_STATS,"STATS","wins_difficult",$STATS_WINS_DIFFICULT)
   IniWrite($GAME_STATS,"STATS","loses_difficult",$STATS_LOSES_DIFFICULT)
   IniWrite($GAME_STATS,"STATS","wins_custom",$STATS_WINS_CUSTOM)
   IniWrite($GAME_STATS,"STATS","loses_custom",$STATS_LOSES_CUSTOM)

   _GDIPlus_GraphicsDispose($GUI_GRAPHIC)
   For $I = 0 To 12
	  _GDIPlus_ImageDispose($IMAGE[$I])
   Next
   _GDIPlus_BrushDispose($GDI_BRUSH)
   Exit
EndFunc

Func _ShowAll()
   For $X = 1 To $FLOOR_WIDTH
	  For $Y = 1 To $FLOOR_HEIGHT
		 If $FIRSTMOVE Then Return
		 If $FLOOR[$X][$Y] <> 9 Then _Show($X,$Y)
	  Next
   Next
EndFunc

While 1
	Sleep(400)
WEnd